{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Services": {
    "DispatchData": "http://data:5500",
    "DispatchInvoice": "http://invoice:5300"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "IpWhitelist": [],
    "EndpointWhitelist": [],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 20
      },
      {
        "Endpoint": "POST:/public/invoices/*/signature",
        "Period": "1h",
        "Limit": 5
      }
    ]
  },
  "AllowedHosts": "*"
}
